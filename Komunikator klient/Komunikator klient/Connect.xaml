<Window x:Class="Komunikator_klient.Connect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Komunikator_klient"
        mc:Ignorable="d"
        Title="Connect" Height="300" Width="300">
    <Grid>
        <Grid.Effect>
            <DropShadowEffect/>
        </Grid.Effect>
        <Button Content="Connect" Margin="96,172,96,75" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" VerticalAlignment="Center" Height="22" HorizontalAlignment="Center" Width="100" BorderBrush="Black" Click="Button_Click"/>
        <TextBox x:Name="TBIP" Height="23" Margin="96,74,96,172" TextWrapping="Wrap" Text="127.0.0.1" VerticalAlignment="Center" TextChanged="TextBox_TextChanged" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Center" Width="100" BorderBrush="Black"/>
        <TextBox x:Name="TBPort" Height="23" TextWrapping="Wrap" Text="54321" VerticalAlignment="Center" Margin="96,115,96,131" TextChanged="TextBox_TextChanged_1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Center" Width="100" BorderBrush="Black"/>

    </Grid>
</Window>
